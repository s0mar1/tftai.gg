{
  "name": "@tft-meta-analyzer/backend",
  "version": "1.0.0",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "scripts": {
    "dev": "pnpm dev:win",
    "dev-debug": "nodemon --exec \"npx tsx --expose-gc src/server.ts\"",
    "dev:win": "nodemon src/server.ts",
    "dev:unix": "nodemon --exec \"../../node_modules/.bin/tsx src/server.ts\"",
    "prebuild": "cd ../shared && npm install && npm run build",
    "build": "npm run prebuild && ./node_modules/.bin/tsc",
    "start": "node --expose-gc dist/server.js",
    "start:dev": "npx tsx src/server.ts",
    "start:direct": "npx tsx src/server.ts",
    "start:tsx": "npx tsx --expose-gc src/server.ts",
    "type-check": "./node_modules/.bin/tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "migrate": "node --import tsx/esm scripts/migrate.ts",
    "migrate:status": "npm run migrate status",
    "migrate:up": "npm run migrate up",
    "migrate:down": "npm run migrate down --force",
    "migrate:dry-run": "npm run migrate dry-run",
    "prepare": "husky"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1",
    "@mongodb-js/mongodb-mcp-server": "^0.0.3",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.54.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.54.0",
    "@opentelemetry/exporter-prometheus": "^0.54.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.54.0",
    "@opentelemetry/instrumentation-express": "^0.44.0",
    "@opentelemetry/instrumentation-fs": "^0.19.0",
    "@opentelemetry/instrumentation-http": "^0.54.0",
    "@opentelemetry/instrumentation-ioredis": "^0.46.0",
    "@opentelemetry/instrumentation-mongoose": "^0.47.0",
    "@opentelemetry/instrumentation-winston": "^0.44.0",
    "@opentelemetry/resources": "^1.30.0",
    "@opentelemetry/sdk-node": "^0.54.0",
    "@opentelemetry/semantic-conventions": "^1.30.0",
    "@tft-meta-analyzer/shared": "workspace:*",
    "axios": "^1.9.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "express-async-errors": "^3.1.1",
    "express-rate-limit": "^7.5.1",
    "helmet": "^8.1.0",
    "ioredis": "^5.6.1",
    "isomorphic-dompurify": "^2.26.0",
    "mongoose": "^8.7.3",
    "node-cache": "^5.1.2",
    "node-cron": "^4.1.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "validator": "^13.15.15",
    "winston": "^3.17.0",
    "zod": "^3.24.1",
    "typescript": "^5.8.3"
  },
  "devDependencies": {
    "@jest/globals": "^30.0.4",
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^30.0.0",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^6.0.3",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.8",
    "@types/validator": "^13.15.2",
    "cross-env": "^7.0.3",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "nodemon": "^3.1.7",
    "supertest": "^7.1.3",
    "ts-jest": "^29.4.0",
    "tsx": "^4.20.3"
  },
  "lint-staged": {
    "*.ts": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
